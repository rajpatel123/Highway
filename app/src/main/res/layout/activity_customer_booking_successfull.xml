<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/yellow"
    tools:context=".customer.CustomerBookingSuccessfullActivity">


    <TextView
        android:id="@+id/text"
        style="@style/zl_text_16_bold"
        android:gravity="center"
        android:layout_above="@+id/subtext"
        android:layout_width="match_parent"
        android:textColor="#7E5D10"
        android:layout_height="wrap_content"
        android:text="Booking Successful"/>


    <TextView
        android:id="@+id/subtext"
        style="@style/zl_text_12_small"
        android:gravity="center"
        android:layout_above="@+id/fmLL"
        android:layout_width="match_parent"
        android:textColor="#7E5D10"
        android:layout_height="wrap_content"
        android:text="We will Contact you shortly"/>

    <FrameLayout
        android:id="@+id/fmLL"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerInParent="true"
        android:background="@drawable/ic_booking_suucess_basebg">
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:srcCompat="@drawable/ic_booking_success_tick"/>
    </FrameLayout>



</RelativeLayout>